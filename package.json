{
  "name": "freelens-ai-extension",
  "version": "1.0.0",
  "description": "FreeLens extension for AI-powered Kubernetes resource analysis",
  "main": "out/main/index.js",
  "renderer": "out/renderer/index.js",
  "files": [
    "out/**/*"
  ],
  "engines": {
    "node": ">= 22.0.0",
    "freelens": "^1.8.0"
  },
  "license": "MIT",
  "scripts": {
    "type:check": "tsc --noEmit -p tsconfig.json --composite false",
    "prebuild": "pnpm type:check",
    "build": "electron-vite build",
    "build:force": "electron-vite build",
    "dev": "electron-vite build --watch",
    "clean": "rm -rf out",
    "pack": "pnpm build && pnpm pack",
    "deploy": "pnpm build && rsync -a --delete out/ ~/.freelens/extensions/freelens-ai-extension/out/ && cp package.json ~/.freelens/extensions/freelens-ai-extension/ && osascript -e 'quit app \"FreeLens\"' && sleep 1 && open -a FreeLens"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.29.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@freelensapp/extensions": "^1.8.0",
    "@types/node": "~22.19.8",
    "@types/react": "^17.0.90",
    "@types/react-dom": "^17.0.26",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^5.1.3",
    "electron-vite": "^5.0.0",
    "mobx": "6.13.7",
    "mobx-react": "7.6.0",
    "react": "17.0.2",
    "react-window": "^1.8.10",
    "sass": "^1.97.3",
    "typescript": "5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-external": "^6.2.2",
    "vite-plugin-sass-dts": "^1.3.35"
  },
  "packageManager": "pnpm@10.28.2",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "ansi_up": "^5.2.1",
    "dompurify": "^3.3.1",
    "reactflow": "^11.11.4"
  }
}
